    <!-- Registration Form -->
    <h2>User Registration</h2>
    <form>
        <!-- Name -->
        <label for="name">Full Name:</label>
        <input type="text" id="name" name="name" placeholder="Enter your full name" required><br><br>

        <!-- Email -->
        <label for="email">Email:</label>
        <input type="email" id="email" name="email" placeholder="Enter your email" required><br><br>

        <!-- Password -->
        <label for="password">Password:</label>
        <input type="password" id="password" name="password" minlength="6" required placeholder="Minimum 6 characters"><br><br>

        <!-- Date -->
        <label for="dob">Date of Birth:</label>
        <input type="date" id="dob" name="dob" required><br><br>

        <!-- Dropdown -->
        <label for="country">Country:</label>
        <select id="country" name="country" required>
            <option value="">Select your country</option>
            <option value="kenya">Kenya</option>
            <option value="uganda">Uganda</option>
            <option value="tanzania">Tanzania</option>
        </select><br><br>

        <!-- Radio Buttons -->
        <label>Gender:</label><br>
        <input type="radio" id="male" name="gender" value="male" required>
        <label for="male">Male</label><br>
        <input type="radio" id="female" name="gender" value="female">
        <label for="female">Female</label><br><br>

        <!-- Checkboxes -->
        <label>Interests:</label><br>
        <input type="checkbox" id="coding" name="interest" value="coding">
        <label for="coding">Coding</label><br>
        <input type="checkbox" id="gaming" name="interest" value="gaming">
        <label for="gaming">Gaming</label><br>
        <input type="checkbox" id="reading" name="interest" value="reading">
        <label for="reading">Reading</label><br><br>

        <!-- Submit -->
        <input type="submit" value="Register">
    </form>

